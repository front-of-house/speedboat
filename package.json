{
  "name": "speedboat",
  "version": "0.0.1",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "NODE_ENV=development rm -rf build && webpack -w",
    "netlify": "NODE_ENV=development netlify dev",
    "build": "NODE_ENV=production rm -rf build && webpack",
    "format": "prettier-standard --format",
    "test": "NODE_ENV=test npx babel-node api/**/__tests__/*.test.ts -x '.ts'",
    "test:db": "docker run --rm -d -p 5433:5432 --name speedboat_test_db postgres:11.1 -c fsync=off",
    "test:db:migrate": "NODE_ENV=test knex-migrate up --cwd db",
    "db:migrate": "knex-migrate up --cwd db",
    "db:redo": "knex-migrate redo --cwd db"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier-standard --format --staged"
    }
  },
  "_moduleAliases": {
    "@": "."
  },
  "dependencies": {
    "@sstack/cookies": "^1.0.0-y.0",
    "@sstack/errors": "^0.1.4",
    "@sstack/helmet": "^0.1.4",
    "@sstack/json": "^0.1.4",
    "@sstack/validate": "^0.1.4",
    "decoders": "^1.22.2",
    "http-errors": "^1.8.0",
    "knex": "^0.21.1",
    "nanoid": "^3.1.10",
    "pg": "^8.2.1",
    "slonik": "^22.4.11",
    "sstack": "^1.0.0-y.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@types/http-errors": "^1.6.3",
    "@types/node": "^14.0.14",
    "@types/slonik": "^22.1.2",
    "babel-loader": "^8.1.0",
    "baretest": "^2.0.0",
    "core-js": "^3.6.5",
    "husky": "^4.2.5",
    "knex-migrate": "^1.7.4",
    "matched": "^5.0.0",
    "module-alias": "^2.2.2",
    "prettier-standard": "^16.3.0",
    "read-env": "^2.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sure-thing/speedboat.git"
  },
